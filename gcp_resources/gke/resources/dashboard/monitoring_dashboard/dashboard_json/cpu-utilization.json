{
  "height": 4,
  "widget": {
    "title": "CPU Utilization Stats",
    "xyChart": {
      "chartOptions": {
        "mode": "STATS"
      },
      "dataSets": [
        {
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_RATE"
              },
              "filter": "metric.type=\"kubernetes.io/container/cpu/core_usage_time\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${JobIndex}"
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 12
},
{
  "height": 4,
  "widget": {
    "title": "CPU Utilization Outliers [MEAN]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "Cluster: $${resource.labels.cluster_name} Job: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/jobset-name} JobIndex: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/job-index}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_RATE"
              },
              "filter": "metric.type=\"kubernetes.io/container/cpu/core_usage_time\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${JobIndex}",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MEAN"
              },
              "secondaryAggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MEAN",
                "groupByFields": [
                  "resource.label.\"cluster_name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/jobset-name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/job-index\""
                ],
                "perSeriesAligner": "ALIGN_NONE"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "yPos": 4
},
{
  "height": 4,
  "widget": {
    "title": "CPU Utilization Outliers [MAX]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "Cluster: $${resource.labels.cluster_name} Job: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/jobset-name} JobIndex: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/job-index}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_RATE"
              },
              "filter": "metric.type=\"kubernetes.io/container/cpu/core_usage_time\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${JobIndex}",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MAX"
              },
              "secondaryAggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MAX",
                "groupByFields": [
                  "resource.label.\"cluster_name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/jobset-name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/job-index\""
                ],
                "perSeriesAligner": "ALIGN_NONE"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "xPos": 6,
  "yPos": 4
},
{
  "height": 8,
  "widget": {
    "collapsibleGroup": {
      "collapsed": false
    },
    "title": "CPU Utilization by TPU Slice"
  },
  "width": 12
}