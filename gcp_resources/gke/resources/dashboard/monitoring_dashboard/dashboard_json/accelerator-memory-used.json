{
  "height": 4,
  "widget": {
    "title": "Accelerator Memory Used Stats",
    "xyChart": {
      "chartOptions": {
        "mode": "STATS"
      },
      "dataSets": [
        {
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_NONE"
              },
              "filter": "metric.type=\"kubernetes.io/container/accelerator/memory_used\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${PodName}"
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 12,
  "yPos": 16
},
{
  "height": 4,
  "widget": {
    "title": "Accelerator Memory Used Outliers [MEAN]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "Cluster: $${resource.labels.cluster_name} Job: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/jobset-name} Pod: $${resource.labels.pod_name}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_NONE"
              },
              "filter": "metric.type=\"kubernetes.io/container/accelerator/memory_used\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${PodName}",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MEAN"
              },
              "secondaryAggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MEAN",
                "groupByFields": [
                  "resource.label.\"cluster_name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/jobset-name\"",
                  "resource.label.\"pod_name\""
                ],
                "perSeriesAligner": "ALIGN_NONE"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "yPos": 20
},
{
  "height": 4,
  "widget": {
    "title": "Accelerator Memory Used Outliers [MAX]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "Cluster: $${resource.labels.cluster_name} Job: $${metadata.user_labels\\.jobset\\.sigs\\.k8s\\.io/jobset-name} Pod: $${resource.labels.pod_name}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_RATE"
              },
              "filter": "metric.type=\"kubernetes.io/container/accelerator/memory_used\" resource.type=\"k8s_container\" $${ClusterName} $${JobName} $${PodName}",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MAX"
              },
              "secondaryAggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MAX",
                "groupByFields": [
                  "resource.label.\"cluster_name\"",
                  "metadata.user_labels.\"jobset.sigs.k8s.io/jobset-name\"",
                  "resource.label.\"pod_name\""
                ],
                "perSeriesAligner": "ALIGN_NONE"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "xPos": 6,
  "yPos": 20
},
{
  "height": 8,
  "widget": {
    "collapsibleGroup": {
      "collapsed": false
    },
    "title": "Accelerator Memory Used by TPU Slice"
  },
  "width": 12,
  "yPos": 16
}
