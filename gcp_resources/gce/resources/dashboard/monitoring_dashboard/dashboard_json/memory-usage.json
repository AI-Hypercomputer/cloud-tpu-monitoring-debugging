{
  "height": 4,
  "widget": {
    "title": "TPU VM - Memory Usage Stats",
    "xyChart": {
      "chartOptions": {
        "mode": "STATS"
      },
      "dataSets": [
        {
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "perSeriesAligner": "ALIGN_NONE"
              },
              "filter": "metric.type=\"tpu.googleapis.com/memory/usage\" resource.type=\"tpu_worker\" resource.label.\"node_id\"=monitoring.regex.full_match(\"${NODE_PREFIX_REGEX}\")"
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 12,
  "yPos": 8
},
{
  "height": 4,
  "widget": {
    "title": "TPU VM - Memory Usage Outliers [MEAN]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "TPU Node: $${resource.labels.node_id} Worker ID: $${resource.labels.worker_id}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MEAN",
                "groupByFields": [
                  "resource.label.\"node_id\"",
                  "resource.label.\"worker_id\""
                ],
                "perSeriesAligner": "ALIGN_MEAN"
              },
              "filter": "metric.type=\"tpu.googleapis.com/memory/usage\" resource.type=\"tpu_worker\" resource.label.\"node_id\"=monitoring.regex.full_match(\"${NODE_PREFIX_REGEX}\")",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MEAN"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "yPos": 12
},
{
  "height": 4,
  "widget": {
    "title": "TPU VM - Memory Usage Outliers [MAX]",
    "xyChart": {
      "chartOptions": {
        "mode": "COLOR"
      },
      "dataSets": [
        {
          "legendTemplate": "TPU Node: $${resource.labels.node_id} Worker ID: $${resource.labels.worker_id}",
          "minAlignmentPeriod": "60s",
          "plotType": "LINE",
          "targetAxis": "Y1",
          "timeSeriesQuery": {
            "timeSeriesFilter": {
              "aggregation": {
                "alignmentPeriod": "60s",
                "crossSeriesReducer": "REDUCE_MAX",
                "groupByFields": [
                  "resource.label.\"node_id\"",
                  "resource.label.\"worker_id\""
                ],
                "perSeriesAligner": "ALIGN_MAX"
              },
              "filter": "metric.type=\"tpu.googleapis.com/memory/usage\" resource.type=\"tpu_worker\" resource.label.\"node_id\"=monitoring.regex.full_match(\"${NODE_PREFIX_REGEX}\")",
              "pickTimeSeriesFilter": {
                "direction": "TOP",
                "numTimeSeries": ${OUTLIER_COUNT},
                "rankingMethod": "METHOD_MAX"
              }
            }
          }
        }
      ],
      "thresholds": [],
      "timeshiftDuration": "0s",
      "yAxis": {
        "label": "",
        "scale": "LINEAR"
      }
    }
  },
  "width": 6,
  "xPos": 6,
  "yPos": 12
},
{
  "height": 8,
  "widget": {
    "collapsibleGroup": {
      "collapsed": false
    },
    "title": "Memory Usage by TPU VM"
  },
  "width": 12,
  "yPos": 8
}